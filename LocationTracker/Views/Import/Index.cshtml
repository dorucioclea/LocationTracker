@model ImportModel
@{
    ViewData["Title"] = "Importeer GPX";
}
<h3>Importeer GPX</h3>

<form asp-action="Upload" enctype="multipart/form-data">
    <div class="form-group row">
        <label for="location" class="col-sm-2 col-form-label">Waypoints</label>
        <div class="col-sm-10">
            <input asp-for="WayPoints" class="form-control" />
        </div>
    </div>

    <div class="form-group row">
        <label for="location" class="col-sm-2 col-form-label">Track</label>
        <div class="col-sm-10">
            <input asp-for="Track" class="form-control" />
        </div>
    </div>

    <button type="submit" class="btn btn-primary">Importeer</button>
</form>